tests {
  test "คฺวาม-วาว", "kʰwaːm˧.waːw˧"
  test "ที่-เปิด-ขวด", "tʰiː˥˩.pɤːt̚˨˩.kʰua̯t̚˨˩"
  test "พื้น-หฺลัง", "pʰɯːn˦˥.laŋ˩˩˦"
  test "สม-ชาย", "som˩˩˦.t͡ɕʰaːj˧"
  test "อ็่อน-ค่อ", "ʔɔn˨˩.kʰɔː˥˩"
