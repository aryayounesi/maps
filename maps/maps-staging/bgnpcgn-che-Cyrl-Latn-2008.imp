stage {

  # RULES
  # note[2]
  sub "(?<!\\b\\u2019)\\b\\u0415", "Ye" # Е in initial position -> Ye
  sub "(?<!\\b\\u2019)\\b\\u0435", "ye" # е in initial position -> ye
  # note[4]
  sub "\\u043A\\u043A\\u0445", "qq"
  sub "\\u043A\\u043A\\u042A", "q̇q̇"
  sub "\\u041A\\u041A\\u0445", "QQ"
  sub "\\u041A\\u041A\\u042A", "Q̇Q̇"


  # CHARACTERS
  parallel {
    sub "\u0410", any("AƏ") # А note[1]
