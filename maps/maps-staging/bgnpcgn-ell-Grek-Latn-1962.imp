  sub "(?<!\\b)\\u03B3[Κκ]", "ng" # γ (before Κ medially)
  sub "\\u0393(?=[ΕεέΗηήΙιίΥυύ])", "Y" # Γ (before front vowels)
  sub "\\u03B3(?=[ΕεέΗηήΙιίΥυύ])", "y" # γ (before front vowels)
  sub "\\u0393(?=[Oo][Ιιί])", "Y" # Γ (before front vowels)
