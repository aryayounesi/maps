map:
  postrules:
    - pattern: (?<=\b)(?<!\b[‘|’|'|-])[\u0061-\uFFFF]
      result: "upcase"
    # don't capitalize defined article in the middle of a sentence
    - pattern : ' At T'  # الت
      result: ' at T'
    - pattern : ' As̄ S̄'  # الث
      result: ' as̄ S̄'
    - pattern : ' Ad D' # الد
      result: ' ad D'
    - pattern : ' Az̄ Z̄'  # الذ
      result: ' az̄ Z̄'
    - pattern : ' Ar R' # الر
      result: ' ar R'
    - pattern : ' Az Z'  # الز
      result: ' az Z'
    - pattern : ' As S' # الس
      result: ' as S'
    - pattern : ' Ash Sh' # الش
      result: ' ash Sh'
    - pattern : ' Aş Ş'  # الص
      result: ' aş Ş'
    - pattern : ' Aẕ Ẕ'  # الض
      result: ' aẕ Ẕ'
    - pattern : ' Aţ Ţ'  # الط
      result: ' aţ Ţ'
    - pattern : ' Az̧ Z̧' # الظ
      result: ' az̧ Z̧'
    - pattern : ' Al L'  # الل
      result: ' al L'
    - pattern : ' An N'  # الن
      result: ' an N'
    - pattern: " Al " # ال
      result: " al "
  characters:
    # special rules

    '\s(?=\u0622\u0628\u064E\u0627\u062F)': '' # space followed by abad is removed
    '\ufdf2': 'Allāh' # See note 5

    # Vowels, Diphthongs, and Diacritical Marks
    '\u064e' : 'a' # َ fatha
    '\u064e(?=\u0629)' : '' # َ fatha followed by ta' marboota
    '\u064e(?=a[h|t])' : '' # َ fatha followed by ta' marboota, handling different order of conversion

    '\u0652' : ''  # ْ sokoon
    '\u0659': 'ê'

    '\u0650[\u064a|\u06cc]' : 'ī' # ـِي kasra followed by ي
    '\u0650' : 'i' # karsra
    '\u06d2' : 'e' # ـے

    '\u0622' : 'ā'  # آ
    '\u064e\u0627' : 'ā'  # ـَا fatha followed by ا
    '\u0627' : 'ā'  # ا
    '\b\u0627' : ''  # ا

    '\u0648' : 'o'  # و # suspect
    '\u064f' : 'u' # ُ damma
    '\u064f\u0648' : 'ū'  # ـُو damma followed by و

    '\u064e\u06d2' : 'ai' # ـے
    '\u064e\u0648' : 'au'  # ـَو
    '\u0670': 'á' # ىٰ
    '\u0649': 'á' # ىٰ

