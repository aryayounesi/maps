  sub "ᆯ" + kor_maybedash + "ᄐ", "l" + ref( 1 ) + "t’" # HANGUL JONGSEONG RIEUL + CHOSEONG THIEUTH
  sub "ᆯ" + kor_maybedash + "ᄑ", "l" + ref( 1 ) + "p’" # HANGUL JONGSEONG RIEUL + CHOSEONG PHIEUPH
